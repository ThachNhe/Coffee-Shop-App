        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      
      ],
      [
         "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "10.16.2",
